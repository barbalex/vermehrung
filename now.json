{
  "version": 2,
  "name": "vermehrung",
  "alias": "vermehrung.apflora.ch",
  "env": {
    "HASURA_GRAPHQL_URL": "https://api.vermehrung.apflora.ch/v1/graphql"
  },
  "routes": [
    {
      "src": "/Vermehrung/.*",
      "dest": "/Vermehrung/index.html"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "404.html"
    }
  ]
}